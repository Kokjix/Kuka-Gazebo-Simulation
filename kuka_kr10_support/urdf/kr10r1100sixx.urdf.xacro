  <!-- Following REP199, this frame shall be use to attach EEF or other equipment -->
  <link name="flange" />

  <!-- ROS base_link to KUKA $ROBROOT coordinate system transform -->
  <link name="world"></link>

  <joint name="dummy" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="world" />
    <child link="base_link" />
  </joint>
  <link name="base" />
  <joint name="base_link-base" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="base" />
  </joint>

  <joint name="joint_a1" type="revolute">
    <origin xyz="0 0 0.400" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="link_1" />
    <axis xyz="0 0 -1" />
    <limit effort="0" lower="-3.14"  upper="3.14" velocity="${radians(300)}" />
  </joint>
  <joint name="joint_a2" type="revolute">
    <origin xyz="0.025 0 0" rpy="0 0 0" />
    <parent link="link_1" />
    <child link="link_2" />
    <axis xyz="0 1 0" />
    <limit effort="0" lower="-3.14"  upper="3.14" velocity="${radians(225)}" />
  </joint>
  <joint name="joint_a3" type="revolute">
    <origin xyz="0.560 0 0" rpy="0 0 0" />
    <parent link="link_2" />
    <child link="link_3" />
    <axis xyz="0 1 0" />
    <limit effort="0" lower="-3.14"  upper="3.14" velocity="${radians(225)}" />
  </joint>
  <joint name="joint_a4" type="revolute">
    <origin xyz="0 0 0.035" rpy="0 0 0" />
    <parent link="link_3" />
    <child link="link_4" />
    <axis xyz="-1 0 0" />
    <limit effort="0" lower="-3.14"  upper="3.14" velocity="${radians(381)}" />
  </joint>
  <joint name="joint_a5" type="revolute">
    <origin xyz="0.515 0 0" rpy="0 0 0" />
    <parent link="link_4" />
    <child link="link_5" />
    <axis xyz="0 1 0" />
    <limit effort="0" lower="-3.14"  upper="3.14" velocity="${radians(311)}" />
  </joint>
  <joint name="joint_a6" type="revolute">
    <origin xyz="0.080 0 0" rpy="0 0 0" />
    <parent link="link_5" />
    <child link="link_6" />
    <axis xyz="-1 0 0" />
    <limit effort="0" lower="-3.14" upper="3.14" velocity="${radians(492)}" />
  </joint>
  <joint name="joint_a6-flange" type="fixed">
    <parent link="link_6" />
    <child link="flange" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <!-- This frame corresponds to the $TOOL coordinate system in KUKA KRC controllers -->
  <link name="tool0" />
  <joint name="flange-tool0" type="fixed">
    <parent link="flange" />
    <child link="tool0" />
    <origin xyz="0 0 0" rpy="0 ${radians(90)} 0" />
  </joint>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <legacyModeNS>true</legacyModeNS>
      <controlPeriod>0.001</controlPeriod>
      <robotNamespace></robotNamespace>
    </plugin>
  </gazebo>


  <!-- arm transmittions-->
  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_a1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

